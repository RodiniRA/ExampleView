<Controls:MetroWindow x:Class="AmTrustExample.MainWindow"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:viewModel="clr-namespace:AmTrustExample.ViewModels"
                      xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
                      Title="AmTrust Manager 4.0"
                      Height="720"
                      Width="1080"
                      WindowStartupLocation="CenterScreen" WindowState="Maximized"
                      Loaded="SetDefaultTheme">
    <Window.Resources>
        <ResourceDictionary>
            <!--<ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/FlatButton.xaml" />
                <ResourceDictionary Source="Ribbon/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>-->

            <x:Array x:Key="WindowCommandsOverlayBehaviorArray"
                     Type="Controls:WindowCommandsOverlayBehavior">
                <Controls:WindowCommandsOverlayBehavior>Always</Controls:WindowCommandsOverlayBehavior>
                <Controls:WindowCommandsOverlayBehavior>Flyouts</Controls:WindowCommandsOverlayBehavior>
                <Controls:WindowCommandsOverlayBehavior>HiddenTitleBar</Controls:WindowCommandsOverlayBehavior>
                <Controls:WindowCommandsOverlayBehavior>Never</Controls:WindowCommandsOverlayBehavior>
            </x:Array>

        </ResourceDictionary>

    </Window.Resources>
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>

    <Controls:MetroWindow.Flyouts>
        <Controls:FlyoutsControl>
            <Controls:Flyout Header="File"
                             Position="Left"
                             Width="400"
                             AnimateOpacity="True"
                             Name="FileFlyout"
                             Style="{DynamicResource FlyoutBackground}">

                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="350"/>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <StackPanel>
                        <TabControl Grid.Row="0" TabStripPlacement="Left">
                            <TabItem Header="Email" Width="200"></TabItem>
                            <TabItem Header="Reference Links" Width="200"></TabItem>
                            <TabItem Header="Admin Tools" Width="200"></TabItem>
                            <TabItem Header="Recent Activity" FontSize="8" Width="200"></TabItem>
                            <TabItem Header="Theme" Width="200">
                                <StackPanel>
                                    <Button Click="SetDefaultTheme">
                                        <StackPanel>
                                            <Rectangle Height="10" Width="20" Fill="#41B1E1"/>
                                            <TextBlock>Blue</TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Button Click="DarkSkin_Click">
                                        <StackPanel>
                                            <Rectangle Height="10" Width="20" Fill="#535353"/>
                                            <TextBlock>Dark</TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Button Click="GreenSkin_Click">
                                        <StackPanel>
                                            <Rectangle Height="10" Width="20" Fill="#238C23"/>
                                            <TextBlock>Green</TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Button Click="GummiBearSkin_Click">
                                        <StackPanel>
                                            <Rectangle Height="10" Width="20" Fill="#ff4a1d"/>
                                            <TextBlock>Gummi Bear</TextBlock>
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </TabItem>
                            <TabItem Header="Style" Width="200"></TabItem>
                            <TabItem Header="About" Width="200"></TabItem>
                        </TabControl>

                        <Button Content="Close" Grid.Row="2"></Button>
                    </StackPanel>
                </Grid>


            </Controls:Flyout>
        </Controls:FlyoutsControl>
    </Controls:MetroWindow.Flyouts>

    <DockPanel Style="{DynamicResource DockPanelStyle}">

        


        <Grid Height="Auto" DockPanel.Dock="Top" Margin="10,10">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
